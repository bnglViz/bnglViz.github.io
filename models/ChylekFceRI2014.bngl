kpLynSyk1	30
kpLynSyk2	100
kfPagLynSH3	8.4e6/(NA*simCellvol)
krPagLynSH3	30
kfPagLynSH3_2	1e3	
kfPagLynSH2	2.5e7/(NA*simCellvol)
kfPagLynSH2_2	1e3
krPagLyn2point	30
kpLynPag	1000
kfCskPag	2.5e4/(NA*simCellvol)
krCskPag	3e-3
kpCskLyn	1000
eff		5
kfRecFyn1	kfRecLyn1
